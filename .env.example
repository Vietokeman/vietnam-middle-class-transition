# Google Gemini API Key
# Lấy key miễn phí tại: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=paste_your_gemini_key_here
VITE_GEMINI_MODEL=gemini-2.5-flash-native-audio-dialog

# Firebase Configuration
# Lấy từ: Firebase Console > Project Settings > Your Apps > Web App
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Backend Server Port (nếu cần)
PORT=3001

# Environment
NODE_ENV=development
